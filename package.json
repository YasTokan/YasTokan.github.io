{
  "name": "multikart",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run multikart:serve-ssr",
    "serve:ssr": "node dist/multikart/server/main.js",
    "build:ssr": "ng build && ng run multikart:server:production",
    "prerender": "ng run multikart:prerender",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/fr.json ./src/assets/i18n/ar.json --format namespaced-json"
  },
  "private": true,
